{
    "compilerOptions": {
        "target": "es2020",
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowJs": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noEmit": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true
    }
}
